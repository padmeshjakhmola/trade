{
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ],
  "functions": {
    "server.js": {
      "maxDuration": 10
    },
    "api/stocks.js": {
      "maxDuration": 10
    },
    "api/stocks/[id].js": {
      "maxDuration": 10
    }
  }
}